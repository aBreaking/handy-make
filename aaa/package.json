{
  "name": "aaa",
  "version": "0.0.1",
  "dependencies": {
    "react": "^16.5.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^3.0.0",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8"
  },
  "scripts": {
    "start": "handy-service serve",
    "build": "handy-service build"
  },
  "presets": {
    "preset": "default",
    "features": [
      "eslint",
      "stateManagement",
      "unit"
    ],
    "eslint": "airbnb",
    "eslintCondition": "commit",
    "state": "mobx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**.js"
    ]
  }
}