{
  "name": "aaa",
  "version": "0.0.1",
  "dependencies": {
    "handy-demo-common": "^0.0.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^3.0.0",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8"
  },
  "devDependencies": {
    "handy-service": "^0.0.4",
    "lint-staged": "^7.3.0",
    "husky": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "scripts": {
    "start": "handy-service serve",
    "build": "handy-service build"
  },
  "presets": {
    "preset": "default",
    "features": [
      "eslint",
      "stateManagement"
    ],
    "eslint": "airbnb",
    "eslintCondition": "commit",
    "state": "mobx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**.js"
    ]
  }
}
